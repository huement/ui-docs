extends ../layout/blankpage

append config
  - var title = 'Dashboard Example | @huement/ui'
  - var metaDesc = 'Admin Dashboard StarterKit'
  - var highlight = true
  - var slug = 'dashboard'
  - var pageCSS = 'css/dashboard'

append scriptsJS
  script(src=path+"js/bundle.js" type="module")
  script(src="https://cdn.jsdelivr.net/npm/chart.js@4.3.2/dist/chart.umd.js" type="module" integrity="sha384-eI7PSr3L1XLISH8JdDII5YN/njoSsxfbrkCTnJrzXt+ENP5MOVBxD+l6sEG4zoLp" crossorigin="anonymous")
  script(src=path+"dashboard.js" type="module")


block content
  .container-fluid
    .row
      .sidebar.border.border-right.col-md-3.col-lg-2.p-0.bg-body-tertiary
        .offcanvas-md.offcanvas-end.bg-body-tertiary#sidebarMenu(tabindex="-1" aria-labelledby="sidebarMenuLabel")
          .offcanvas-header
            h5.offcanvas-title#sidebarMenuLabel Company name
            button.btn-close(type="button" data-bs-dismiss="offcanvas" data-bs-target="#sidebarMenu" aria-label="Close")
          .offcanvas-body.d-md-flex.flex-column.p-0.pt-lg-3.overflow-y-auto
            ul.nav.flex-column
              li.nav-item
                a.nav-link.d-flex.align-items-center.gap-2.active(aria-current="page" href="#")
                  svg.bi
                    use(href="#house-fill")
                  | Dashboard

              li.nav-item
                a.nav-link.d-flex.align-items-center.gap-2(href="#")
                  svg.bi
                    use(href="#file-earmark")
                  | Orders

              li.nav-item
                a.nav-link.d-flex.align-items-center.gap-2(href="#")
                  svg.bi
                    use(href="#cart")
                  | Products

              li.nav-item
                a.nav-link.d-flex.align-items-center.gap-2(href="#")
                  svg.bi
                    use(href="#people")
                  | Customers

              li.nav-item
                a.nav-link.d-flex.align-items-center.gap-2(href="#")
                  svg.bi
                    use(href="#graph-up")
                  | Reports

              li.nav-item
                a.nav-link.d-flex.align-items-center.gap-2(href="#")
                  svg.bi
                    use(href="#puzzle")
                  | Integrations

            h6.sidebar-heading.d-flex.justify-content-between.align-items-center.px-3.mt-4.mb-1.text-body-secondary.text-uppercase
              span Saved reports
              a.link-secondary(href="#" aria-label="Add a new report")
                svg.bi
                  use(href="#plus-circle")
            ul.nav.flex-column.mb-auto
              li.nav-item
                a.nav-link.d-flex.align-items-center.gap-2(href="#")
                  svg.bi
                    use(href="#file-earmark-text")
                  | Current month

              li.nav-item
                a.nav-link.d-flex.align-items-center.gap-2(href="#")
                  svg.bi
                    use(href="#file-earmark-text")
                  | Last quarter

              li.nav-item
                a.nav-link.d-flex.align-items-center.gap-2(href="#")
                  svg.bi
                    use(href="#file-earmark-text")
                  | Social engagement

              li.nav-item
                a.nav-link.d-flex.align-items-center.gap-2(href="#")
                  svg.bi
                    use(href="#file-earmark-text")
                  | Year-end sale

            hr.my-3
            ul.nav.flex-column.mb-auto
              li.nav-item
                a.nav-link.d-flex.align-items-center.gap-2(href="#")
                  svg.bi
                    use(href="#gear-wide-connected")
                  | Settings

              li.nav-item
                a.nav-link.d-flex.align-items-center.gap-2(href="#")
                  svg.bi
                    use(href="#door-closed")
                  | Sign out

      main.col-md-9.ms-sm-auto.col-lg-10.px-md-4
        .d-flex.justify-content-between.flex-wrap.flex-md-nowrap.align-items-center.pt-3.pb-2.mb-3.border-bottom
          h1.h2 Dashboard
          .btn-toolbar.mb-2.mb-md-0
            .btn-group.me-2
              button.btn.btn-sm.btn-outline-secondary(type="button") Share
              button.btn.btn-sm.btn-outline-secondary(type="button") Export
            button.btn.btn-sm.btn-outline-secondary.dropdown-toggle.d-flex.align-items-center.gap-1(type="button")
              svg.bi
                use(href="#calendar3")
              | This week

        canvas.my-4.w-100#myChart(width="900" height="380")
        h2 Section title
        .table-responsive.small
