      .row
        .col-12.my-3.py-3
          table.table
            thead.table-dark
              tr
                th(style="width:150px") Heading
                th Example
            tbody
              tr
                td(style="width:150px")
                  p
                    code.highlighter-rouge &lt;h1&gt; &lt;/h1&gt;
                td
                  span.h1 h1. some cool title
              tr
                td
                  p
                    code.highlighter-rouge &lt;h2&gt; &lt;/h2&gt;
                td
                  span.h2 h2. some cool title
