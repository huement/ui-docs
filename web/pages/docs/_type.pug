extends ../../layout/doc

append config
  - var title = 'Mojo Typography Laboratory'
  - var metaDesc = 'Testing out the SCSS Library with an easy to use testbed'
  - var slug = 'type'
  - var icon = 'gear'
  - var parent = 'style-guide'

append scriptsJS
  script(src=path+"js/bundle.js" + stamp, type="module")

