    "color-scale": "^1.1.0",
    "copyfiles": "^2.4.1",
    "dotenv": "^16.3.1",
    "fs-extra": "^11.2.0",
    "fs-jetpack": "^5.1.0",
    "glob": "^10.3.10",
    "json2scss": "^0.0.2",
    "json2scss-map": "^1.5.0",
    "jsonfile-updater": "^3.1.0",
