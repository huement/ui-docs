{
  "name": "@huement/ui",
  "codeName": "mojo",
  "version": "0.5.0",
  "type": "module",
  "sourceType": "module",
  "license": "MPL-2.0",
  "author": "Derek Scott <derek@huement.com>",
  "homepage": "https://github.com/huement/ui",
  "buildNumber": 57,
  "buildDate": "Mon Sep 23 2024",
  "debugMode": true,
  "scripts": {
    "build": "npx npm-run-all -lsc pug move build:css",
    "build:css": "npx npm-run-all -lsc scss postfix",
    "build:js": "node bin/bundle.cjs",
    "clean": "rimraf _temp && rimraf dist && npm run clean:index",
    "clean:index": "rimraf ./index.html",
    "dev": "nodemon -x \"npm run build\" & npx browser-sync start --config browser-sync.cjs",
    "move": "node ./bin/move.cjs",
    "postfix": "postcss --config postcss.config.cjs --replace dist/css/docs.css",
    "pug": "node bin/pug.cjs --build=true && npm run move",
    "scss": "sass --verbose --no-quiet --load-path=node_modules --style expanded --source-map --embed-sources --no-error-css web/assets/style:dist/css"
  },
  "nodemonConfig": {
    "verbose": true,
    "watch": [
      "web"
    ],
    "ext": "scss,html,ts,json,cjs,pug",
    "ignore": [
      ".cache/**"
    ],
    "delay": 1500
  },
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
